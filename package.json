{
  "name": "jquery.photocols",
  "version": "2.0.0",
  "type": "module",
  "description": "Photocols: An animated photo gallery with scrolling columns and rows for jQuery. Supports vertical/horizontal scrolling, variable speeds, lazy loading.",
  "main": "dist/jquery.photocols.js",
  "module": "dist/jquery.photocols.esm.js",
  "types": "jquery.photocols.d.ts",
  "files": [
    "dist/",
    "src/",
    "jquery.photocols.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/jquery.photocols.esm.js",
      "require": "./dist/jquery.photocols.js",
      "types": "./jquery.photocols.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "unpkg": "dist/jquery.photocols.min.js",
  "jsdelivr": "dist/jquery.photocols.min.js",
  "browser": "dist/jquery.photocols.min.js",
  "sideEffects": false,
  "author": "Dailos Medina <dailos@2coders.com>",
  "contributors": [
    {
      "name": "Fernando Torcelly",
      "email": "torcelly@2coders.com"
    },
    {
      "name": "David Santana",
      "email": "david@2coders.com"
    }
  ],
  "homepage": "https://github.com/2CodersTeam/jquery.photocols",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2CodersTeam/jquery.photocols.git"
  },
  "bugs": {
    "url": "https://github.com/2CodersTeam/jquery.photocols/issues"
  },
  "keywords": [
    "jquery",
    "jquery-plugin",
    "gallery",
    "photo",
    "animation",
    "columns",
    "rows",
    "responsive",
    "masonry",
    "grid",
    "parallax",
    "lazy-loading",
    "horizontal-scroll",
    "vertical-scroll"
  ],
  "peerDependencies": {
    "jquery": ">=1.7.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.54.0",
    "rollup-plugin-filesize": "^10.0.0",
    "serve": "^14.2.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "build:watch": "rollup -c -w",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
